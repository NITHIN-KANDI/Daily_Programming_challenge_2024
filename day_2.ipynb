   "source": [
    "def find_missing(arr):\n",
    "    n = len(arr) + 1  # Since one number is missing\n",
    "    total_sum = n * (n + 1) // 2\n",
    "    arr_sum = sum(arr)\n",
    "    return total_sum - arr_sum\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
